@model UniqueTodoApplication.DTOs.CategoryDto
@{
    Layout = "_AdminDashboardLayout";
}

<section id="main-content">
    <section class="wrapper">
        <h3><i class="fa fa-angle-right"></i>Details</h3>
        <dl class="row">
            <dt class="col-sm-2">
                Name:
            </dt>
            <dd class="col-sm-10">
                @Model.Name
            </dd>
            <dt class="col-sm-2">
                Description:
            </dt>
            <dd class="col-sm-10">
                @Model.Description
            </dd>
        </dl>

        <button style="background-color:aqua;"><a asp-action="Index">Back to List</a></button>
    </section>
</section>
